{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "k5_lite_t2v_5s_sft_sd.yaml",
            "type": "python",
            "request": "launch",
            "program": "./inference.py",
            "console": "integratedTerminal",
            "env": {
                "PYTORCH_CUDA_ALLOC_CONF": "expandable_segments:True"
            },
            "args": [
                "--config",
                "configs/k5_lite_t2v_5s_sft_sd.yaml",
                "--limit",
                "5",
                "--offload",
                "--qwen_quantization",
                "--width",
                "512",
                "--height",
                "512",
                "--sample_steps",
                "50",
                "--video_duration",
                "5",
                "--magcache"
            ],
            "python": "${command:python.interpreterPath}"
        }
    ]
}